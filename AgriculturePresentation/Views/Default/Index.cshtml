@using EntityLayer.Concrete
@model List<Service>
@{
    ViewData["Title"] = "Index";
}

<h1>Hizmetler Listesi</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
        </tr>
    }

</table>

