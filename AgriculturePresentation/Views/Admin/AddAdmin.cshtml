@model EntityLayer.Concrete.Admin


@{
    ViewData["Title"] = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Admin Ekleme Sayfası</h1>

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Admin Girişi</h4>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label for="exampleInputName1" asp-for="Username">Kullanıcı Adı</label>
                    <input type="text" class="form-control" placeholder="Kullanıcı Adı" asp-for="Username">
                </div> <div class="form-group">
                    <label for="exampleInputName1" asp-for="Password">Parola</label>
                    <input type="password" class="form-control" placeholder="Parola" asp-for="Password">
                </div>

                <button type="submit" class="btn btn-outline-info">Kaydet</button>
            </form>
        </div>
    </div>
</div>